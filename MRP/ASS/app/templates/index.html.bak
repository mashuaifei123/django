<!DOCTYPE html>
{% load static %}
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=Edge，chrome=1">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>首页</title>
  <!--[if lt IE 9]>
  <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
  <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
  <![endif]-->
</head>

<body class="skin-default-dark fixed-layout">
    <div class="preloader">
        <div class="loader">
            <div class="loader__figure"></div>
            <p class="loader__label">载入中</p>
        </div>
    </div>
    <div id="main-wrapper">
        <header class="topbar">
            <nav class="navbar top-navbar navbar-expand-md navbar-dark">
                <div class="navbar-header">
                    <a class="navbar-brand" href="/">
                        <b>
                            <img src="/static/img/logo.png" class="dark-logo">
                        </b>
                    </a>
                </div>
                <div class="navbar-collapse">
                    <table class="table text-center mb-0">
                        <tbody>
                            <tr>
                                <td class="border-top-0"><a href="/" class="h5 nav-btn text-dark pb-2 border-bottom-3 border-info">&nbsp;图表&nbsp;</a></td>
                                <!--<td class="border-top-0"><a href="/pick/" class="h5 nav-btn text-dark">&nbsp;自选股票&nbsp;</a></td>-->
                                <!--<td class="border-top-0"><a href="/hold/" class="h5 nav-btn text-dark">&nbsp;模拟持股&nbsp;</a></td>-->
                                <!--<td class="border-top-0"><a href="/predict/" class="h5 nav-btn text-dark">&nbsp;短期预测&nbsp;</a></td>-->
                            </tr>
                        </tbody>
                    </table>
                </div>
            </nav>
        </header>
    </div>
    <script src = "../static/JS/echarts.min.js"></script>
    <div id="main" style="width: 600px;height: 400px;"></div>
    <script type="text/javascript">
    var myChar = echarts.init(document.getElementById('main'));
    var option = {
        title:{
                text:'温度',
                subtext: '温度',
                x: 'center'
              },
        xAxis:{
                type: 'category',
                data:[{% for x in  rew_date %}
                      '{{  x.rew_date|safe }}',
                      {% endfor %}]},
        yAxis:{
                type: 'value',
                },
        series:{
                type: 'line',
                data:[{% for y in rew_wd %}
                            {{ y.rew_wd|safe }},
                        {% endfor %}]}
                };
    myChar.setOption(option);</script>
    <script src="/static/js/jquery-3.2.1.min.js"></script>
    <script src="/static/js/jquery.freezeheader.js"></script>
    <script src="/static/js/echarts.min.js"></script>
    <script src="/static/js/bootstrap.min.js"></script>
    <script src="/static/js/pages.js"></script>
    <script src="/static/js/stockchart.js"></script>
</body>
</html>